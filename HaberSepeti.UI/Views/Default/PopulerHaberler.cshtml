@model IEnumerable<HaberSepeti.Data.Model.Haber>
@{
    Layout = null;
}


<div class="card my-4">
    <h5 class="card-header">Popüler haberler</h5>
    <div class="card-body">
        <div class="row">
            <div class="col-lg-12">
                <ul>
                    @foreach (var item in Model)
                    {
                        <li><a href="#">@item.Baslik </a>(@item.Okunma)</li>
                    }
                </ul>
            </div>

        </div>
    </div>
</div>