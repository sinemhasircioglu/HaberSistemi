@model IEnumerable<HaberSepeti.Data.Model.Yorum>
@{
    ViewBag.Title = "SonYorumlar";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="card my-4">
    <h5 class="card-header">Son yorumlar</h5>
    <div class="card-body">
        <div class="row">
            <div class="col-lg-12">
                <ul>
                    @foreach (var item in Model)
                    {
                        <li>@item.Icerik</li>
                    }
                </ul>
            </div>

        </div>
    </div>
</div>
